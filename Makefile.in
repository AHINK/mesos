# Makefile.

SHELL = '/bin/sh'

default: all

src:
	$(MAKE) -C src

install: src
	$(error unimplemented)

uninstall:
	$(error unimplemented)

dist:
	$(error unimplemented)

all: src

doxygen:
	doxygen

etags:
	find src -name "*.cpp" -o -name "*.hpp" | grep -v "\~$$" | xargs etags -o src/TAGS

clean:
	$(MAKE) -C src clean

.PHONY: default src install uninstall dist all clean
